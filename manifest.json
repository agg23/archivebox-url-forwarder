{
  "manifest_version": 2,
  "name": "archivebox-url-forwarder",
  "version": "0.1",
  "description": "archivebox-url-forwarder description",
  "permissions": ["*://192.168.1.241/*", "tabs"],
  "icons": {
    "64": "icon.png"
  },
  "browser_action": {
    "default_title": "archivebox-url-forwarder (browserAction)",
    "default_icon": {
      "64": "icon.png"
    }
  },
  "background": {
    "scripts": ["dist/background.js"]
  }
}
